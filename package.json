{
  "name": "vite-vue3-ts-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "check-css": "npx stylelint \"src/**/*.{scss,css,vue}\"",
    "format-css": "npx stylelint \"src/**/*.{scss,css,vue}\" --fix",
    "lint-check": "npx eslint src --ext .js,.ts,tsx",
    "lint-fix": "npx eslint src --ext .js,.ts,tsx --fix",
    "test-jest": "npx jest",
    "test-e2e": "testcafe chrome \"src/tests/e2e/*.e2e.test.ts\" --app \"npm run dev\" --app-init-delay 100 -S -s screenshots"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "dayjs": "^1.10.7",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.5.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.1",
    "postcss-scss": "^4.0.1",
    "sass": "^1.45.1",
    "stylelint": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "testcafe": "^1.18.6",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
