{
  "name": "vite-vue3-ts-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check-css": "npx stylelint \"src/**/*.{scss,css,vue}\"",
    "format-css": "npx stylelint \"src/**/*.{scss,css,vue}\" --fix",
    "lint-check": "npx eslint src --ext .js,.ts,tsx",
    "lint-fix": "npx eslint src --ext .js,.ts,tsx --fix",
    "test-jest": "npx jest",
    "test-e2e": "testcafe chrome \"src/tests/e2e/*.e2e.test.ts\" --app \"npm run dev\" --app-init-delay 100 -S -s screenshots"
  },
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=10.5.0"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "axios": "^0.26.1",
    "dayjs": "^1.10.7",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12"
  },
  "resolutions": {
    "esbuild": "npm:esbuild-wasm@latest"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.5.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.8.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.1",
    "postcss-scss": "^4.0.1",
    "sass": "^1.45.1",
    "stylelint": "^14.0.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "testcafe": "^2.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
